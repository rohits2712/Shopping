name: dockercompose8806827969705493182
services:
  shopping.api:
    build:
      context: C:\Users\sharoroh\source\repos\Shopping
      dockerfile: Shopping.API/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8000"
    image: shoppingapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        published: "80"
        protocol: tcp
  shopping.client:
    build:
      context: C:\Users\sharoroh\source\repos\Shopping
      dockerfile: Shopping.Client/Dockerfile
    image: shoppingclient
    networks:
      default: null
  shopping.db:
    container_name: shoppingdb
    image: mongo
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: /site
        target: /data/db
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose8806827969705493182_default